{
	"name": "svelte-kit-todo-app-with-tailwind",
	"version": "0.0.1",
	"scripts": {
		"dev:only": "svelte-kit dev",
		"build:only": "svelte-kit build",
		"preview": "svelte-kit preview",
		"tailwind:watch": "cross-env TAILWIND_MODE=watch cross-env NODE_ENV=development postcss src/app.css -o src/output.css -w",
		"tailwind:build": "cross-env TAILWIND_MODE=build cross-env NODE_ENV=production postcss src/app.css -o src/output.css",
		"dev": "concurrently \"npm run dev:only\" \"npm run tailwind:watch\"",
		"build": "npm run tailwind:build && npm run build:only"
	},
	"devDependencies": {
		"@rgossiaux/svelte-headlessui": "^1.0.0-beta.12",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-netlify": "^1.0.0-next.62",
		"@sveltejs/kit": "^1.0.0-next.345",
		"@types/p2": "^0.7.39",
		"@zerodevx/svelte-toast": "^0.7.1",
		"autoprefixer": "^10.4.2",
		"concurrently": "^7.0.0",
		"cross-env": "^7.0.3",
		"eslint": "^7.32.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"netlify-plugin-js-obfuscator": "^1.0.21",
		"postcss": "^8.4.5",
		"postcss-cli": "^9.1.0",
		"postcss-load-config": "^3.1.1",
		"sass": "^1.53.0",
		"svelte": "^3.44.0",
		"svelte-preprocess": "^4.10.7",
		"svelte-simple-datatables": "^0.2.3",
		"tailwindcss": "^3.0.21"
	},
	"type": "module",
	"dependencies": {
		"@supabase/supabase-js": "^1.28.6",
		"@syncedstore/core": "^0.3.7",
		"@syncedstore/svelte": "^0.3.6",
		"bcrypt": "^5.0.1",
		"fitty": "^2.3.6",
		"gridjs": "^5.0.2",
		"gridjs-svelte": "^2.1.1",
		"muuri": "^0.9.5",
		"p2": "^0.7.1",
		"svelte-table": "^0.3.4",
		"svelte-tailwind-table": "^0.10.0",
		"textfit": "^2.4.0",
		"y-webrtc": "^10.2.2",
		"yjs": "^13.5.34"
	}
}
